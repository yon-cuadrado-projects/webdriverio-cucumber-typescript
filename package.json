{
    "name": "cucumber-webdriverio",
    "version": "0.1.0",
    "main": "wdio.conf.js",
    "keywords": [],
    "author": "Yon Cuadrado <yon.cuadrado@gmail.com>",
    "license": "ISC",
    "description": "Example of testing project that includes cucumber-js, page object model, webdriverio and allure",
    "scripts": {
        "test:local:chrome": "wdio ./test/cucumber/configuration/js/local-conf.js --browser=chrome --maxInstances=1",
        "test:local:chrome:1Test": "wdio run ./test/cucumber/configuration/local-conf.js --spec \"test/cucumber/features/actions-in-browser.feature\" --browser=chrome --maxInstances=1",
        "test:local:chrome:1TestwithServer": "wdio run ./test/cucumber/configuration/local-conf.js --spec \"test/cucumber/features/actions-in-browser.feature\" --browser=chrome --maxInstances=1 --storeInDatabase=true",
        "test:generate:steps": "wdio ./test/cucumber/configuration/js/local-conf.js --generateSteps --browser=chrome",
        "getStepsSnippetsWithCucumber": "./node_modules/.bin/cucumber-js --format 'snippets' --format-options '{\"snippetInterface\": \"synchronous\" }' --dry-run --no-strict ",
        "update-package-json-dependencies": "npx ncu -u && npm i",
        "install-chromedriver": "selenium-standalone install --drivers.chrome.version=83.0.4103.39 --drivers.chrome.baseURL=https://chromedriver.storage.googleapis.com --drivers.firefox.version=0.26.0 --drivers.ie.version=0.26.0 --drivers.ie.baseURL=http://selenium-release.storage.googleapis.com",
        "test:report": "allure serve ./report/allure-results",
        "clean": "read -p \"Are you sure you want to clean the project? [y/n] \" REPLY; if [[ $REPLY =~ ^[Yy]$ ]]; then rm -R .git .github demo-app test .codeclimate.yml .travis.yml jest.json wdio.BUILD.conf.js src/features/**; fi",
        "test:lint": "eslint 'src/**/*.js' 'test/**/*.js!(setup.js)'",
        "generate:eslintrc": "npx tslint-to-eslint-config",
        "eslint-fix-all": "npx eslint test/**/*.ts --fix",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
    },
    "devDependencies": {
        "@cucumber/cucumber": "8.0.0",
        "@types/chai": "4.3.1",
        "@types/chai-as-promised": "7.1.5",
        "@types/fs-extra": "9.0.13",
        "@types/jquery": "3.5.14",
        "@types/long": "4.0.1",
        "@types/node": "17.0.24",
        "@types/reflect-metadata": "0.1.0",
        "@types/yargs": "17.0.10",
        "@typescript-eslint/eslint-plugin": "5.19.0",
        "@typescript-eslint/eslint-plugin-tslint": "5.19.0",
        "@typescript-eslint/parser": "5.19.0",
        "@typescript-eslint/typescript-estree": "5.19.0",
        "@wdio/cli": "7.19.5",
        "@wdio/config": "7.19.5",
        "@wdio/cucumber-framework": "7.19.5",
        "@wdio/dot-reporter": "7.19.5",
        "@wdio/local-runner": "7.19.5",
        "@wdio/mocha-framework": "7.19.5",
        "@wdio/reporter": "7.19.5",
        "@wdio/selenium-standalone-service": "7.19.5",
        "@wdio/spec-reporter": "7.19.5",
        "@wdio/types": "7.19.5",
        "axios": "0.26.1",
        "chai": "4.3.6",
        "chai-as-promised": "7.1.1",
        "cucumber-messages": "8.0.0",
        "dotenv": "16.0.0",
        "eslint": "8.13.0",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-config-standard": "16.0.3",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsdoc": "39.2.2",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-promise": "6.0.0",
        "eslint-plugin-standard": "5.0.0",
        "eslint-plugin-wdio": "7.19.4",
        "exceljs": "4.3.0",
        "forever": "4.0.3",
        "fs-extra": "10.1.0",
        "http-server": "14.1.0",
        "husky": "7.0.4",
        "import": "0.0.6",
        "lint-staged": "12.3.8",
        "merge": "2.1.1",
        "npm-check-updates": "12.5.9",
        "npm-run-all": "4.1.5",
        "nyc": "15.1.0",
        "prettier": "2.6.2",
        "reflect-metadata": "0.1.13",
        "selenium-standalone": "8.0.10",
        "stylelint": "14.7.1",
        "stylelint-config-standard": "25.0.0",
        "stylelint-order": "5.0.0",
        "stylelint-scss": "4.2.0",
        "ts-node": "10.7.0",
        "tsconfig-paths": "3.14.1",
        "tslint-eslint-rules": "5.4.0",
        "tslint-to-eslint-config": "2.12.3",
        "tsyringe": "4.6.0",
        "typescript": "4.6.3",
        "typescript-tslint-plugin": "1.0.1",
        "wdio-allure-reporter": "0.8.3",
        "wdio-reporter-html": "0.0.21",
        "webdriverio": "7.19.5",
        "yargs": "17.4.1"
    },
    "dependencies": {
        "cd": "0.3.3",
        "eslint-plugin-autofix": "^1.1.0",
        "save-dev": "^0.0.1-security"
    }
}
