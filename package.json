{
    "name": "cucumber-webdriverio",
    "version": "0.1.0",
    "main": "wdio.conf.js",
    "keywords": [],
    "author": "Yon Cuadrado <yon.cuadrado@gmail.com>",
    "license": "ISC",
    "description": "Example of testing project that includes cucumber-js, page object model, webdriverio and allure",
    "scripts": {
        "test:local:chrome": "wdio ./test/cucumber/configuration/js/local-conf.js --browser=chrome --maxInstances=1",
        "test:local:chrome:1Test": "wdio run ./test/cucumber/configuration/js/local-conf.js --spec \"test/cucumber/features/actions-in-browser.feature\" --browser=chrome --maxInstances=1",
        "test:generate:steps": "wdio ./test/cucumber/configuration/js/local-conf.js --generateSteps --browser=chrome",
        "getStepsSnippetsWithCucumber": "./node_modules/.bin/cucumber-js --format 'snippets' --format-options '{\"snippetInterface\": \"synchronous\" }' --dry-run --no-strict ",
        "update-package-json-dependencies": "npx ncu -u && npm i",
        "install-chromedriver": "selenium-standalone install --drivers.chrome.version=83.0.4103.39 --drivers.chrome.baseURL=https://chromedriver.storage.googleapis.com --drivers.firefox.version=0.26.0 --drivers.ie.version=0.26.0 --drivers.ie.baseURL=http://selenium-release.storage.googleapis.com",
        "test:report": "allure serve ./report/allure-results",
        "clean": "read -p \"Are you sure you want to clean the project? [y/n] \" REPLY; if [[ $REPLY =~ ^[Yy]$ ]]; then rm -R .git .github demo-app test .codeclimate.yml .travis.yml jest.json wdio.BUILD.conf.js src/features/**; fi",
        "test:lint": "eslint 'src/**/*.js' 'test/**/*.js!(setup.js)'",
        "generate:eslintrc": "npx tslint-to-eslint-config",
        "eslint-fix-all": "npx eslint test/**/*.ts --fix",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
    },
    "devDependencies": {
        "@cucumber/cucumber": "7.3.1",
        "@types/chai": "4.2.21",
        "@types/chai-as-promised": "7.1.4",
        "@types/fs-extra": "9.0.12",
        "@types/jquery": "3.5.6",
        "@types/long": "4.0.1",
        "@types/node": "16.7.6",
        "@types/reflect-metadata": "0.1.0",
        "@types/yargs": "17.0.2",
        "@typescript-eslint/eslint-plugin": "4.29.3",
        "@typescript-eslint/eslint-plugin-tslint": "4.29.3",
        "@typescript-eslint/parser": "4.29.3",
        "@typescript-eslint/typescript-estree": "4.29.3",
        "@wdio/cli": "7.11.0",
        "@wdio/config": "7.10.1",
        "@wdio/cucumber-framework": "7.11.0",
        "@wdio/dot-reporter": "7.10.1",
        "@wdio/local-runner": "7.11.0",
        "@wdio/mocha-framework": "7.11.0",
        "@wdio/reporter": "7.10.1",
        "@wdio/selenium-standalone-service": "7.10.1",
        "@wdio/spec-reporter": "7.10.1",
        "@wdio/types": "7.10.1",
        "axios": "0.21.1",
        "chai": "4.3.4",
        "chai-as-promised": "7.1.1",
        "cucumber-html-report-generator": "0.0.8",
        "cucumber-messages": "8.0.0",
        "dotenv": "10.0.0",
        "eslint": "7.32.0",
        "eslint-config-airbnb-base": "14.2.1",
        "eslint-config-prettier": "8.3.0",
        "eslint-config-standard": "16.0.3",
        "eslint-plugin-import": "2.24.2",
        "eslint-plugin-jsdoc": "36.0.8",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-prettier": "3.4.1",
        "eslint-plugin-promise": "5.1.0",
        "eslint-plugin-standard": "5.0.0",
        "eslint-plugin-wdio": "7.4.2",
        "exceljs": "4.3.0",
        "forever": "4.0.1",
        "fs-extra": "10.0.0",
        "http-server": "13.0.1",
        "husky": "7.0.2",
        "import": "0.0.6",
        "lint-staged": "11.1.2",
        "merge": "2.1.1",
        "npm-check-updates": "11.8.3",
        "npm-run-all": "4.1.5",
        "nyc": "15.1.0",
        "prettier": "2.3.2",
        "reflect-metadata": "0.1.13",
        "selenium-standalone": "7.1.0",
        "stylelint": "13.13.1",
        "stylelint-config-standard": "22.0.0",
        "stylelint-order": "4.1.0",
        "stylelint-scss": "3.20.1",
        "ts-node": "10.2.1",
        "tsconfig-paths": "3.11.0",
        "tslint-eslint-rules": "5.4.0",
        "tslint-to-eslint-config": "2.9.2",
        "tsyringe": "4.6.0",
        "typescript": "4.4.2",
        "typescript-tslint-plugin": "1.0.1",
        "wdio-allure-reporter": "0.8.3",
        "wdio-cucumberjs-json-reporter-with-typescript": "^4.0.3",
        "webdriverio": "7.11.0",
        "yargs": "17.1.1"
    },
    "dependencies": {}
}
